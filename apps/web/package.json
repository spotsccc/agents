    "@tanstack/vue-virtual": "^3.13.18",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-vue-next": "^0.543.0",
    "reka-ui": "^2.5.0",
    "supabase": "workspace:*",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "vee-validate": "^4.15.1",
    "vite-svg-loader": "^5.1.0",
