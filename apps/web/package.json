{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --build --force tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,js,json,css}\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.1",
    "@tailwindcss/vite": "^4.1.13",
